&lt;dd&gt;Local Number (excluding extension)&lt;/dd&gt;
&lt;dt&gt;&lt;b&gt;extension&lt;/b&gt;&lt;/dt&gt;
&lt;dd&gt;Phone number extension&lt;/dd&gt;
&lt;dt&gt;&lt;b&gt;rawInput&lt;/b&gt;&lt;/dt&gt;
&lt;dd&gt;Raw, unfiltered data input by customer&lt;/dd&gt;
&lt;/dl&gt;
</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" name="phoneType" type="vin:PhoneType" />
          <xsd:element minOccurs="0" name="countryCode" type="xsd:string" />
          <xsd:element minOccurs="1" name="areaCode" type="xsd:string" />
          <xsd:element minOccurs="1" name="localNumber" type="xsd:string" />
